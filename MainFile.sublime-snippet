package main

import (
    "flag"
    "fmt"
    "log"
)

const (
    hw = "hello, world"
)

var (
    flagConfigPath = flag.String("-config", "path/to/config", "")
)

func main() {
    flag.Parse()

    if err := run(); err != nil {
        log.Fatal(err)
    }
}

func run() error {
    fmt.Println(hw)

    return nil
}
